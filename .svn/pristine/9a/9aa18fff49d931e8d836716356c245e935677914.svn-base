/* Empty. Add your own CSS if you like */
.box{
    width: 100%;
    height: 160px;
}
img{
    max-width:100%;
}
.depts{height: 34px;position: relative;overflow-x: scroll;white-space: nowrap;overflow-y:hidden;}
.depts>label{display: inline-block;width:25%;height: 34px;line-height: 34px;border-bottom:0px solid #38c;}
.person-box{width: 100%;height: 100%;position: relative;top: 0px;}
.list-item-title{position: relative;top: 0px;min-height: 34px;}
::-webkit-scrollbar{width:0px}
