html,body{height: 100%;width: 100%;font-size: 14px;}
.content{width: 100%;height: 100%;padding-top: 20px;padding-bottom: 20px;}
.border-theme-df{border: 1px solid #E7E7E7;}
.border-width-1{border-width:1px;}
.border-width-2{border-width:2px;}
.border-bottom{border-right:none;border-left:none;border-top:none; }
.border-right{border-left:none;border-top:none;border-bottom:none; }
.border-left{border-right:none;border-top:none;border-bottom:none; }
.border-top{border-right:none;border-left:none;border-bottom:none; }
.theme-bg{background-color: #EEEEEE;color: #337ab7;}
.line-magin{margin-top: 10px;}
.from-box{width: 100%;height: 100%;}
input[type="text"]{max-width: 80%;border-radius: 3px;border: 1px solid #ccc;height: 34px;}

.my-link{padding: 0px;margin: 0px;font-size: inherit;}
.dra_dialog{position: fixed;top: 0%;left: 30%;right: 30%;width: 40%;height: auto;background-color: #fff;border-radius:10px;text-align:center;z-index:1005;color:#000;}
.dialog-show{top:15%;}

.likeImg{width: 480px;height: 320px;position: absolute;z-index: 1000;}


::-webkit-scrollbar {
    width: 14px;
    height: 20px;
}
::-moz-scrollbar {
    width: 12px;
    height: 20px;
}
::-ms-scrollbar {
    width: 12px;
    height: 20px;
}
::-o-scrollbar {
    width: 12px;
    height: 20px;
}
::scrollbar {
    width: 14px;
    height: 20px;
}


::-webkit-scrollbar-track,
::-webkit-scrollbar-thumb {
    border-radius: 999px;
    border: 5px solid transparent;
}
::-moz-scrollbar-track,
::-moz-scrollbar-thumb {
    border-radius: 999px;
    border: 5px solid transparent;
}
::-o-scrollbar-track,
::-o-scrollbar-thumb {
    border-radius: 999px;
    border: 5px solid transparent;
}
::-ms-scrollbar-track,
::-ms-scrollbar-thumb {
    border-radius: 999px;
    border: 5px solid transparent;
}
::scrollbar-track,
::scrollbar-thumb {
    border-radius: 999px;
    border: 5px solid transparent;
}

::-webkit-scrollbar-track {
    box-shadow: 1px 1px 2px rgba(56,126,245,.4) inset;
}
::-moz-scrollbar-track {
    box-shadow: 1px 1px 2px rgba(56,126,245,.4) inset;
}
::-ms-scrollbar-track {
    box-shadow: 1px 1px 2px rgba(56,126,245,.4) inset;
}
::-o-scrollbar-track {
    box-shadow: 1px 1px 2px rgba(56,126,245,.4) inset;
}
::scrollbar-track {
    box-shadow: 1px 1px 2px rgba(56,126,245,.4) inset;
}

::-webkit-scrollbar-thumb {
    min-height: 20px;
    background-clip: content-box;
    box-shadow: 0 0 0 5px rgba(56,126,245,.7) inset;
}
::-moz-scrollbar-thumb {
    min-height: 20px;
    background-clip: content-box;
    box-shadow: 0 0 0 5px rgba(56,126,245,.7) inset;
}
::-ms-scrollbar-thumb {
    min-height: 20px;
    background-clip: content-box;
    box-shadow: 0 0 0 5px rgba(56,126,245,.7) inset;
}
::-o-scrollbar-thumb {
    min-height: 20px;
    background-clip: content-box;
    box-shadow: 0 0 0 5px rgba(13,66,26,.7) inset;
}
::scrollbar-thumb {
    min-height: 20px;
    background-clip: content-box;
    box-shadow: 0 0 0 5px rgba(56,126,245,.7) inset;
}

::-webkit-scrollbar-corner {
    background: transparent;
}
::-moz-scrollbar-corner {
    background: transparent;
}
::-o-scrollbar-corner {
    background: transparent;
}
::-ms-scrollbar-corner {
    background: transparent;
}
::scrollbar-corner {
    background: transparent;
}
.table > tbody > tr > td{padding: 5px;text-align: center;}
.table > thead > tr > th{padding: 5px;text-align: center;}
.pagination {float: right;}
.dialog-show{top:15%;}
.doc-detail-box{width: 100%;height: auto;border-bottom: 1px solid RGBA(56,126,245,0.4);padding-bottom: 5px;margin-bottom: 10px;}
.doc-detail-box>.title{display: block;width: 100%;height: 40px;padding-left: 13px;font-size: 20px;font-weight: bolder;text-overflow: ellipsis; white-space: nowrap;overflow: hidden;}
.doc-detail-box>.passage{display:inline-block;width: 48%;height: 30px;padding-left: 13px;text-overflow:ellipsis;white-space: nowrap;overflow: hidden;text-indent: 2em;}
.doc-detail-box>.time{display: inline-block;width: 30%;height: 30px;text-align: center;}
.doc-detail-box>.review{display: inline-block;width: 18.8%;text-align: right;}

.docContent{max-height: 70%;overflow-x:hidden;overflow-y:auto;width: 100%;}
.dra_img{max-width: 100%;height: auto;position: relative;}
p{font-size: 16px;line-height: 25px;letter-spacing: 1px;}
.likeImg{width: 300px;height: 200px;position: absolute;z-index: 1000;}

.input-group{display: block;}
.input-group>.form-control{display: inline-block;max-width: 200px;}
.input-group>.input-group-addon{display: inline-block;cursor: pointer;height: 34px;padding-left: 10px;padding-right: 10px;width: auto;line-height: 20px;}
.input-group-addon:hover{background-color: #ccc;}
.doc-title{max-width: 400px;text-overflow:ellipsis;white-space: nowrap;overflow: hidden;}
